.o-top .o-top-number {
  background-color: #f00;
  color: #fff;
  font-size: 24rpx;
  height: 47rpx;
  line-height: 47rpx;
  padding: 5rpx 15rpx;
  border-radius: 30rpx;
  text-align: center;
  transform: translate(200rpx, 80rpx);
}
.o-center {
  display: flex;
  align-items: center;
  justify-content: space-around;
  border-bottom: 1rpx solid #e5e5e5;
}
.o-center view {
  text-align: center;
  box-sizing: border-box;
  width: 33.33%;
  line-height: 54rpx;
  font-size: 26rpx;
}
.o-center .active {
  border-bottom: 6rpx solid #f00;
  font-weight: 700;
  text-shadow: 2rpx 2rpx 2rpx #eee;
}

.form_group {
  width: 90%;
  margin: 0 auto;
  margin-top: 18rpx;
  margin-bottom: 18rpx;
  flex-direction: row;
  justify-content: space-between;
  line-height: 60rpx;
  border: 1px solid #ddd;
  border-radius:6rpx;
  
}

.placeholder_style {
  font-family: '微软雅黑';
  color: #adadad;
  font-size: 14px;
  width: 230rpx;
  margin: auto;
}
.form_group text{
  height: 92rpx;
  line-height: 92rpx;
  font-size: 30rpx;
  color: #171717;
  padding-left:25rpx;

}
.form_group input {
  text-align: right;
  width: 230rpx;
  font-size: 14px;
  height: 50px;
  margin: auto;
}
.tishi{width: 90%;margin: 0 auto;font-size: 24rpx;color: #979797;line-height: 72rpx;}
.tishi image{width: 36rpx;height:36rpx;vertical-align: middle;margin-right:10rpx;
}
.form-price {
  border-bottom: 10px solid #EEE
}

.activity-price {
  line-height: 50px;
  border-bottom: 10px solid #EEE;
  margin:auto;
  font-size: 28rpx;
  padding-left: 35rpx;
}


.activity-price text {
  top: 50%;
}

.ar_book {
  background: #9dd79a;
  font-size: 32rpx;
  color: #fff;
  border-radius: 15rpx;
  margin: auto;
  width: 90%;
  margin-top:35rpx;
}



.ar_select{
  width: 90%;
  height: 160rpx;
  margin: 0 auto;
  margin-top: 54rpx;
  }
  .ar_select_li{
    width: 33.33%;
    background: #fff;
    text-align: center;
    font-size: 22rpx;
    color: #989898;
  }
  .ar_select_li view{
    height: 60rpx;
  }
  .ar_select_li view image{
     width: 34rpx;
     height: 42rpx;
  }
  .hide{display: none;}
 