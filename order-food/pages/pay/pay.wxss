.displayFlex{
  display: flex;
  flex-wrap:wrap;
}
.o-top {
  height: 141rpx;
  border-bottom: 1rpx solid #eaeaea;
  padding: 10rpx 10rpx 20rpx;
  display: flex;
  border-top: 10px solid #eee
}
.o-top-mainLisy{width: 90%;margin: 0 auto;}
.o-top-main{
  line-height: 141rpx;
  margin:0 auto;
  justify-content: space-between;
  }
  .o-top-main-left{
     line-height: 141rpx;
  }
.o-top-img {
  width: 110rpx;
  height: 110rpx;
  display:block;
  margin-top:15rpx;
  margin-right: 16rpx;
}
.text{
  font-size: 28rpx;
  color:#383838;
  display: inline-block;
  }
.textcode{ line-height: 30rpx;margin-top:-37rpx;
}
.o-top-main-right{
  width: 95rpx;
  text-align: center;
  font-size: 22rpx;
  color: #7d7d7d;
}
.main-right-img{
  width: 50rpx;
  height:50rpx;
  border-radius: 50%;
  margin-top: 24rpx;
}


